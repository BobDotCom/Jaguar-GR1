<?xml version="1.0"?>

<PropertyList>
  <name>Jaguar GR1 Wingman</name>
  <scenario>
    <description>
      4 wingman
      This scenario formates 4 Jaguar GR1 to the parent ac at the position
      designated by the x,y,z offsets.
    </description>

   
    <entry>
      <name>WING-2</name>
      <type>wingman</type>
      <model>Aircraft/Jaguar-Gr1/Models/Jaguar-GR1_wingman.xml</model>
      <x-offset>-30</x-offset>
      <y-offset>-30</y-offset>
      <z-offset>0</z-offset>
      <formate>true</formate>
      <max-speed-kts>500</max-speed-kts>
      <coefficients>
        <heading>5.0</heading>
        <pitch>4.0</pitch>
        <bank>4.0</bank>
        <speed>2.5</speed>
      </coefficients>
    </entry>

