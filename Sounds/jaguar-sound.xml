	<!-- flyby -->
		<flyby>
			<name>fly-by</name>
			<mode>looped</mode>
			<path>Aircraft/Jaguar/Sounds/flyby.wav</path>
			<condition>
			<and>
	    <greater-than>
	     	<property>velocities/mach</property>
	     	<value>0.8</value>
	    </greater-than>
				<equals>
					<property>sim/current-view/view-number</property>
					<value>6</value>
				</equals>
				</and>
				<not>
         	<property>/sim/current-view/internal</property>
	 	<property>/engines/engine/out-of-fuel</property>
     	</not>
		</condition>
			<!-- <volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.001</factor>
			</volume>
			-->
		</flyby>

		 <!-- Sonic Boom -->
  <sonicboom>
   <name>Sonic Boom</name>
	<mode>once</mode>
   <path>Aircraft/Jaguar/Sounds/sonicboom.wav</path>
   <condition>
   <and>
	    <greater-than>
	     	<property>/velocities/mach</property>
	     	<value>1.0</value>
	    </greater-than>
	    <equals>
		    <property>/sim/current-view/internal</property>
		    <value>false</value>
		</equals>
	</and>
   </condition>
    <reference-dist>600.0</reference-dist>
    <max-dist>6000.0</max-dist>
  </sonicboom>



	<voice>
  <name>Geforce-breath-vox</name>
  <path>Aircraft/Jaguar/Sounds/Geforce.wav</path>
  <condition>
   <greater-than>
    <property>/accelerations/pilot/z-accel-fps_sec</property>
    <value>-161</value>
   </greater-than>
  </condition>
<volume>
<factor>0.9</factor>
</volume>
 </voice>

<voice>
  <name>Geforce-vox</name>
  <path>Aircraft/Jaguar/Sounds/geforce1.wav</path>
  <condition>
   <less-than>
    <property>/accelerations/pilot/z-accel-fps_sec</property>
    <value>-225</value>
   </less-than>
  </condition>
<volume>
<factor>0.9</factor>
</volume>
 </voice>

       <canopy>
            <name>canopy</name>
            <path>Aircraft/Jaguar/Sounds/canopy.wav</path>
            <mode>in-transit</mode>
            <property>canopy/position-norm</property>
            <!-- auto transmit -->
            <reference-dist>2</reference-dist>
            <max-dist>4</max-dist>
            <position>
                <x>-1.0</x>
                <y>0.01</y>
                <z>0.195</z>
            </position>
            <pitch>
                <offset>1.25</offset>
                <factor>-0.25</factor>
            </pitch>
            <volume>
                <max>0.05</max>
            </volume>
        </canopy>

        <canopy>
            <name>canopy_lock</name>
            <path>Aircraft/Jaguar/Sounds/canopy_lock.wav</path>
            <mode>once</mode>
            <condition>
                <equals>
                    <property>canopy/position-norm</property>
                    <!-- auto transmit -->
                    <value>0.0</value>
                </equals>
            </condition>
            <reference-dist>2</reference-dist>
            <max-dist>4</max-dist>
            <position>
                <x>-1.0</x>
                <y>0.01</y>
