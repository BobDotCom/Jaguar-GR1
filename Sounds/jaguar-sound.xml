<PropertyList>

  <fx>

      <!-- Engine start inside Left  -->

      <engine>
          <name>engine-start-inside-L</name>
          <mode>once</mode>
          <path>Aircraft/Jaguar/Sounds/startup.wav</path>
          <condition>
            <and>
    <greater-than>
       <property>engines/engine/n2</property>
       <value>0.5</value>
      </greater-than>
      <less-than>
                    <property>engines/engine/running</property>
                    <value>1</value>
                </less-than>
                <not>
                 <property>/sim/current-view/external</property>
                </not>
              </and>
    </condition>
          <volume>
            <property>engines/engine/n1</property>
            <factor> 0.09 </factor>
            <min> 0.3 </min>
            <max> 0.5 </max>
          </volume>
          <pitch>
            <property>engines/engine/n2</property>
            <factor> 0.01 </factor>
    	<min> 0.0 </min>
    	<max> 0.9 </max>
          </pitch>
          <reference-dist>100</reference-dist>
          <max-dist>2000</max-dist>
        </engine>
                                <!-- Engine start inside Right  -->

      <engine>
          <name>engine-start-inside-R</name>
          <mode>once</mode>
          <path>Aircraft/Jaguar/Sounds/startup.wav</path>
          <condition>
            <not>
             <property>/sim/current-view/external</property>
            </not>
            <and>
    <greater-than>
       <property>engines/engine[1]/n2</property>
       <value>0.5</value>
      </greater-than>
      <less-than>
                    <property>engines/engine[1]/running</property>
                    <value>1</value>
                </less-than>
              </and>
    </condition>
          <volume>
            <property>engines/engine[1]/n1</property>
            <factor> 0.09 </factor>
            <min> 0.3 </min>
            <max> 0.5 </max>
          </volume>
          <pitch>
            <property>engines/engine[1]/n2</property>
            <factor> 0.01 </factor>
    	<min> 0.0 </min>
    	<max> 0.9 </max>
          </pitch>
          <reference-dist>100</reference-dist>
          <max-dist>2000</max-dist>
        </engine>


    <!-- Whine -->

       <engine>
         <name>turbine-0</name>
         <mode>looped</mode>
         <path>Aircraft/Jaguar/Sounds/Jaguar-whine.wav</path>
         <condition>
           <property>/engines/engine[0]/running</property>
           <not>
             <property>/sim/current-view/internal</property>
           </not>
         </condition>
         <orientation>
           <x> -1 </x>
           <y> 0 </y>
           <z> 0 </z>
           <inner-angle>90</inner-angle>
           <outer-angle>240</outer-angle>
           <outer-gain>5</outer-gain>
         </orientation>
         <volume>
           <property>engines/engine[0]/n2</property>
           <factor>0.09</factor>
           <min>0.1</min>
           <max>0.6</max>
         </volume>
         <pitch>
           <property>engines/engine[0]/n2</property>
           <factor>0.01</factor>
         	<min>0.5</min>
   	      <max>0.8</max>
         </pitch>
       </engine>

       <engine>
         <name>turbine-1</name>
         <mode>looped</mode>
         <path>Aircraft/Jaguar/Sounds/Jaguar-whine.wav</path>
         <condition>
           <property>/engines/engine[1]/running</property>
           <not>
             <property>/sim/current-view/internal</property>
           </not>
         </condition>
         <orientation>
           <x> -1 </x>
           <y> 0 </y>
           <z> 0 </z>
           <inner-angle>90</inner-angle>
           <outer-angle>240</outer-angle>
           <outer-gain>5</outer-gain>
         </orientation>
         <volume>
           <property>engines/engine[1]/n2</property>
           <factor>0.09</factor>
           <min>0.1</min>
           <max>0.6</max>
         </volume>
         <pitch>
           <property>engines/engine[1]/n2</property>
           <factor>0.01</factor>
         	<min>0.5</min>
   	      <max>0.8</max>
         </pitch>
       </engine>

       <engine>
           <name>engine</name>
           <path>Aircraft/Jaguar/Sounds/jet-blast.wav</path>
           <mode>looped</mode>
           <condition>
             <property>engines/engine[0]/running</property>
     		<not>
             	<property>/sim/current-view/internal</property>
     		<property>engines/engine/out-of-fuel</property>
          	</not>
           </condition>
            <position>
         <x>18.6</x>
     	<y>-0.4</y>
     	<z>2.37</z>
        </position>
        <orientation>
     	<x>1.0</x>
         <y>0.0</y>
         <z>0</z>
         <inner-angle>35.0</inner-angle>
         <outer-angle>90</outer-angle>
         <outer-gain>0.45</outer-gain>
        </orientation>

        <volume>
         <property>engines/engine[0]/n1</property>
         <factor>0.014</factor>
         <offset>-0.45</offset>
        </volume>

        <reference-dist>20</reference-dist>
        <max-dist>6000</max-dist>
         </engine>

     	<engine>
           <name>engine1</name>
           <path>Aircraft/Jaguar/Sounds/jet-blast.wav</path>
           <mode>looped</mode>
           <condition>
             <property>engines/engine[1]/running</property>
     		<not>
             	<property>/sim/current-view/internal</property>
     		<property>engines/engine/out-of-fuel</property>
          	</not>
           </condition>
            <position>
         <x>18.6</x>
     	<y>0.4</y>
     	<z>2.37</z>
        </position>
        <orientation>
     	<x>1.0</x>
         <y>0.0</y>
         <z>0</z>
         <inner-angle>35.0</inner-angle>
         <outer-angle>90</outer-angle>
         <outer-gain>0.45</outer-gain>
        </orientation>

        <volume>
         <property>engines/engine[1]/n1</property>
         <factor>0.014</factor>
         <offset>-0.45</offset>
        </volume>

        <reference-dist>20</reference-dist>
        <max-dist>6000</max-dist>
         </engine>

     <!-- Internal -->

       <engine>
         <name>whine-int-L</name>
         <path>Aircraft/Jaguar/Sounds/engine-int.wav</path>
         <mode>looped</mode>
         <condition>
           <property>engines/engine[0]/running</property>
   		        <not>
           	<property>/sim/current-view/external</property>
   		      <property>engines/engine/out-of-fuel</property>
        	     </not>
         </condition>
         <volume>
           <property>engines/engine[0]/n2</property>
           <factor type="double">0.0025</factor>
         </volume>
         <pitch>
           <property>/engines/engine[0]/n2</property>
           <factor>0.025</factor>
           <offset>-0.1</offset>
         </pitch>
       </engine>

       <engine>
         <name>whine-int-R</name>
         <path>Aircraft/Jaguar/Sounds/engine-int.wav</path>
         <mode>looped</mode>
         <condition>
           <property>engines/engine[1]/running</property>
   		        <not>
           	<property>/sim/current-view/external</property>
   		      <property>engines/engine/out-of-fuel</property>
        	     </not>
         </condition>
         <volume>
           <property>engines/engine[1]/n2</property>
           <factor type="double">0.0025</factor>
         </volume>
         <pitch>
           <property>/engines/engine[1]/n2</property>
           <factor>0.025</factor>
           <offset>-0.1</offset>
         </pitch>
       </engine>









		<!-- ********************************************************************************* -->
		<!--                                 GEARS                                             -->
		<!-- ********************************************************************************* -->

		<rumble>
			<name>gear rumble</name>
			<mode>looped</mode>
			<path>Aircraft/Jaguar/Sounds/ground_roll.wav</path>
			<condition>
				<property>sim/current-view/internal</property>
				<or>
					<property>gear/gear[1]/wow</property>
					<property>gear/gear[2]/wow</property>
				</or>
			</condition>
			<reference-dist>30</reference-dist>
			<max-dist>100</max-dist>
			<position>
				<x>1.2</x>
				<y>0.0</y>
				<z>-0.6</z>
			</position>
			<volume>
				<property>velocities/groundspeed-kt</property>
				<factor>.004</factor>
			</volume>
		</rumble>

    <gear>
			<name>gear</name>
			<mode>in-transit</mode>
			<path>Aircraft/Jaguar/Sounds/gear-transit.wav</path>
			<property>/gear/gear/position-norm</property>
			<volume>
				<factor>0.5</factor>
			</volume>
			<pitch>
				<internal>dt_play</internal>
				<offset>1.0</offset>
				<factor>-0.001</factor>
			</pitch>
		</gear>

    <gear>
			<name>gear-lock</name>
			<path>Aircraft/Jaguar/Sounds/gear-lock.wav</path>
			<condition>
				<equals>
					<property>/gear/gear/position-norm</property>
					<value>0</value>
				</equals>
			</condition>
			<volume>
				<factor>0.5</factor>
			</volume>
		</gear>

    <gear>
	 	<name>gear-open</name>
			<path>Aircraft/Jaguar/Sounds/gear-open.wav</path>
			<condition>
				<equals>
					<property>/gear/gear/position-norm</property>
					<value>1</value>
				</equals>
			</condition>
			<volume>
				<factor>0.5</factor>
			</volume>
		</gear>

		<squeal>
			<name>squeal</name>
			<path>Aircraft/Jaguar/Sounds/touchdown.wav</path>
			<condition>
				<not>
					<property>sim/current-view/internal</property>
				</not>
				<or>
					<property>gear/gear[1]/wow</property>
					<property>gear/gear[2]/wow</property>
				</or>
			</condition>
			<reference-dist>150</reference-dist>
			<max-dist>500</max-dist>
			<position>
				<x>1.2</x>
				<y>0.0</y>
				<z>-0.6</z>
			</position>
			<volume>
				<property>velocities/speed-down-fps</property>
				<factor>0.05</factor>
			</volume>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.01</factor>
			</volume>
			<volume>
				<internal>dt_stop</internal>
				<factor>0.05</factor>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>velocities/airspeed-kt</property>
				<factor>0.0025</factor>
				<offset>1.2</offset>
			</pitch>
		</squeal>

    <squeal>
      <name>squeal</name>
      <path>Sounds/squeal.wav</path>
      <condition>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </or>
      </condition>
      <volume>
        <property>/velocities/speed-down-fps</property>
        <factor>0.05</factor>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.01</factor>
      </volume>
      <volume>
        <internal>dt_stop</internal>
        <factor>0.05</factor>
        <max>1.0</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.0025</factor>
        <offset>1.2</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </squeal>


     <throttle-1>
      <name>detent1</name>
            <path>Aircraft/Jaguar/Sounds/detent.wav</path>
            <mode>once</mode>
            <type>fx</type>
            <reference-dist>0</reference-dist>
            <max-dist>2</max-dist>
            <condition>
                <and>
                    <equals>
                        <property>fdm/jsbsim/fcs/throttle-pos-norm</property>
                        <value>0</value>
                    </equals>
                </and>
            </condition>
            <position>
                <x>-3.75</x>
                <y>-0.50</y>
                <z>0.015</z>
            </position>
            <volume>
                <factor>0.25</factor>
            </volume>
    </throttle-1>

    <throttle-2>
      <name>detent2</name>
            <path>Aircraft/Jaguar/Sounds/detent.wav</path>
            <mode>once</mode>
            <reference-dist>0</reference-dist>
            <max-dist>2</max-dist>
            <condition>
                <and>
                    <greater-than>
                        <property>fdm/jsbsim/fcs/throttle-pos-norm</property>
                        <value>0</value>
                    </greater-than>
                    <less-than>
                        <property>engines/engine/augmentation</property>
                        <value>96</value>
                    </less-than>
                </and>
            </condition>
            <position>
                <x>-3.50</x>
                <y>-0.50</y>
                <z>0.015</z>
            </position>
            <volume>
                <factor>0.25</factor>
            </volume>
    </throttle-2>

    <throttle-3>
      <name>detent3</name>
            <path>Aircraft/Jaguar/Sounds/detent.wav</path>
            <mode>once</mode>
            <reference-dist>0</reference-dist>
            <max-dist>2</max-dist>
            <condition>
                <and>
                    <greater-than>
                        <property>engines/engine/augmentation</property>
                        <value>0</value>
                    </greater-than>
                    <less-than>
                        <property>fdm/jsbsim/fcs/throttle-pos-norm</property>
                        <value>95</value>
                    </less-than>
                </and>
            </condition>
            <position>
                <x>-3.50</x>
                <y>-0.50</y>
                <z>0.015</z>
            </position>
            <volume>
                <factor>0.25</factor>
            </volume>
    </throttle-3>

    <throttle-4>
      <name>detent4</name>
            <path>Aircraft/Jaguar/Sounds/detent.wav</path>
            <mode>once</mode>
            <reference-dist>0</reference-dist>
            <max-dist>2</max-dist>
            <condition>
                <and>
                    <greater-than-equals>
                        <property>engines/engine/augmentation</property>
                        <value>95</value>
                    </greater-than-equals>
                </and>
            </condition>
            <position>
                <x>-3.50</x>
                <y>-0.50</y>
                <z>0.015</z>
            </position>
            <volume>
                <factor>0.25</factor>
            </volume>
    </throttle-4>

    <flap>
      <name>flaps</name>
      <mode>in-transit</mode>
      <path>Aircraft/Jaguar/Sounds/flap.wav</path>
      <property>/surface-positions/flap-pos-norm</property>
            <pitch>
                <offset>0.55</offset>
                <factor>-0.25</factor>
            </pitch>
      <volume>
        <max>0.75</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </flap>

    <slats>
      <name>slats</name>
      <mode>in-transit</mode>
      <path>Aircraft/Jaguar/Sounds/flap.wav</path>
      <property>fdm/jsbsim/fcs/slats-deg</property>
      <volume>
        <max>0.45</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </slats>

    <speedbrakes>
      <name>speedbrakes</name>
      <mode>in-transit</mode>
      <path>Aircraft/Jaguar/Sounds/flap.wav</path>
      <property>fdm/jsbsim/fcs/speedbrake-pos-deg</property>
      <volume>
        <max>0.30</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </speedbrakes>

    <speedbrakes-wind>
      <name>speedbrakes-wind</name>
      <mode>loop</mode>
      <path>Aircraft/Jaguar/Sounds/speedbrake-wind.wav</path>
               <type>avionics</type>
            <condition>
                <property>sim/current-view/internal</property>
                <greater-than>
                    <property>fdm/jsbsim/fcs/speedbrake-pos-norm</property>
                    <value>0.5</value>
                </greater-than>
            </condition>
            <volume>
                <property>velocities/airspeed-kt</property>
                <factor>0.001</factor>
            </volume>
    </speedbrakes-wind>

    <guns>
      <name>gunfire</name>
      <mode>looped</mode>
      <path>Aircraft/Jaguar/Sounds/gunfire.wav</path>
      <condition>
        <property>controls/armament/trigger</property>
        <property>ai/submodels/submodel/count</property>
      </condition>
      <volume>
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </guns>

      <stall>
        <name>stall</name>
        <mode>looped</mode>
        <path>Aircraft/Jaguar/Sounds/stall.wav</path>
        <condition>
            <less-than>
                <property>velocities/airspeed-kt</property>
                <value>175</value>
            </less-than>
            <not>
                <property>/gear/gear[0]/wow</property>
                <property>/gear/gear[1]/wow</property>
                <property>/gear/gear[2]/wow</property>
                <property>/surface-positions/flap-pos-norm</property>
                <property>/gear/gear[0]/position-norm</property>
            </not>
        </condition>
        <volume>
            <max>0.5</max>
        </volume>
        <position>
            <x>-23</x>
            <y>0</y>
            <z>1</z>
        </position>
        <reference-dist>1.0</reference-dist>
        <max-dist>2.0</max-dist>
    </stall>

    <stall>
        <name>stall</name>
        <mode>looped</mode>
        <path>Aircraft/Jaguar/Sounds/stall.wav</path>
        <condition>
            <less-than>
                <property>velocities/airspeed-kt</property>
                <value>150</value>
            </less-than>
            <not>
                <property>/gear/gear[0]/wow</property>
                <property>/gear/gear[1]/wow</property>
                <property>/gear/gear[2]/wow</property>
                <property>/gear/gear[0]/position-norm</property>
            </not>
        </condition>
        <volume>
            <max>0.5</max>
        </volume>
        <position>
            <x>-22</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>1.0</reference-dist>
        <max-dist>2.0</max-dist>
    </stall>

