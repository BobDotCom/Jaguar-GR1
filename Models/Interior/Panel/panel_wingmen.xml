<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>Full-panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>

    <!-- Objets transparents -->
  </animation>

  

  <model>
    <path>Aircraft/Jaguar/Models/Interior/Panel/3D-Models/Drag-Chute.xml</path>
    <offsets>
      <x-m> -0.295 </x-m>
      <y-m> -0.010 </y-m>
      <z-m> -0.010 </z-m>
      <pitch-deg>0</pitch-deg>
	  <heading-deg>0</heading-deg>
    </offsets>
  </model>



 <!-- engine outlets -->
 
 <model>
	 <name>engine_outlet_1</name>
	 <path>Aircraft/Jaguar/Models/Interior/Panel/3D-Models/engine_outlet_1.xml</path>
	 
	 <offsets>
		 <x-m>-0.090</x-m>
		 <y-m> 0.000</y-m>
		 <z-m> 0.060</z-m>
		 <pitch-deg>0</pitch-deg>
	 </offsets>
 </model>
 
 <model>
	 <name>engine_outlet_2</name>
	 <path>Aircraft/Jaguar/Models/Interior/Panel/3D-Models/engine_outlet_2.xml</path>
	 
	 <offsets>
		 <x-m>-0.090</x-m>
		 <y-m> 0.960</y-m>
		 <z-m> 0.060</z-m>
		 <pitch-deg>0</pitch-deg>
	 </offsets>
 </model>
  

  <model>
    <path>Aircraft/Jaguar/Models/Interior/Panel/3D-Models/Pylons.xml</path>
    <offsets>
      <x-m>  0.023 </x-m>
      <y-m> -0.025 </y-m>
      <z-m>  0.040 </z-m>
      <roll-deg> 0 </roll-deg>
      <yaw-deg>  0 </yaw-deg>
    </offsets>
  </model>


  <model>
    <path>Aircraft/Jaguar/Models/Interior/Panel/3D-Models/seat.ac</path>
    <offsets>
      <x-m> -2.700 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.190 </z-m>
	  <heading-deg>0</heading-deg>
	  <roll-deg>0</roll-deg>
	  <pitch-deg>-10</pitch-deg>
    </offsets>
  </model>

<model>
  
  <path>Aircraft/Jaguar/Models/Interior/Panel/Instruments/bits/t-handle.xml</path>
  <offsets>
      <x-m> -3.340 </x-m>
      <y-m>  0.315 </y-m>
      <z-m> -0.020 </z-m>
   <pitch-deg>0</pitch-deg>
  </offsets>
 </model>

 <model> 
  <path>Aircraft/Jaguar/Models/Interior/Panel/3D-Models/tailhook.xml</path>
  <offsets>
      <x-m>0.000 </x-m>
      <y-m>0.000</y-m>
      <z-m>0.000 </z-m>
      <roll-deg>0</roll-deg>
  </offsets>
 </model>

 
</PropertyList>

