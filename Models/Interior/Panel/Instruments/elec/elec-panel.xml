<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
	<path>elec-panel.ac</path>
	
	<animation>
		<type>scale</type>
		<object-name>BATTsw</object-name>
		<object-name>TR1sw</object-name>
		<object-name>TR2sw</object-name>
		<object-name>ALT1sw</object-name>
		<object-name>ALT2sw</object-name>
		<object-name>PitotHeatSw</object-name>
		<object-name>Plane</object-name>
		<x-offset>0.8</x-offset>
		<y-offset>0.8</y-offset>
		<z-offset>0.8</z-offset>
	</animation>
	
	<animation>
		<object-name>BATTsw</object-name>
		<type>knob</type>
		<property>/controls/electrical/battery-switch</property>
		<factor>-30</factor>
		<drag-scale-px>5</drag-scale-px>
		<center>
			<x-m>0.054366</x-m>
			<y-m>-0.077141</y-m>
			<z-m>0.079196</z-m>
		</center>
		<axis>
			<y>1</y>
		</axis>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/battery-switch</property>
                <min>-1</min>
                <max>0</max>
            </binding>
        </action>
	</animation>
	
	<animation>
		<object-name>TR1sw</object-name>
		<type>knob</type>
		<property>/controls/electrical/tru-1-switch</property>
		<factor>-30</factor>
		<drag-scale-px>5</drag-scale-px>
		<center>
			<x-m>0.054366</x-m>
			<y-m>-0.043664</y-m>
			<z-m>0.079196</z-m>
		</center>
		<axis>
			<y>1</y>
		</axis>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/tru-1-switch</property>
                <min>-1</min>
                <max>1</max>
            </binding>
        </action>
		<release>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/tru-1-switch</property>
                <step>0</step>
                <min>-1</min>
                <max>0</max>
            </binding>
        </release>
	</animation>
	
	<animation>
		<object-name>TR2sw</object-name>
		<type>knob</type>
		<property>/controls/electrical/tru-2-switch</property>
		<factor>-30</factor>
		<drag-scale-px>5</drag-scale-px>
		<center>
			<x-m>0.054366</x-m>
			<y-m>-0.018664</y-m>
			<z-m>0.079196</z-m>
		</center>
		<axis>
			<y>1</y>
		</axis>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/tru-2-switch</property>
                <min>-1</min>
                <max>1</max>
            </binding>
        </action>
		<release>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/tru-2-switch</property>
                <step>0</step>
                <min>-1</min>
                <max>0</max>
            </binding>
        </release>
	</animation>
	
	<animation>
		<object-name>ALT1sw</object-name>
		<type>knob</type>
		<property>/controls/electrical/alternator-1-switch</property>
		<factor>-30</factor>
		<drag-scale-px>5</drag-scale-px>
		<center>
			<x-m>0.054366</x-m>
			<y-m>0.021336</y-m>
			<z-m>0.079196</z-m>
		</center>
		<axis>
			<y>1</y>
		</axis>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/alternator-1-switch</property>
                <min>-1</min>
                <max>1</max>
            </binding>
        </action>
		<release>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/alternator-1-switch</property>
                <step>0</step>
                <min>-1</min>
                <max>0</max>
            </binding>
        </release>
	</animation>
	
	<animation>
		<object-name>ALT2sw</object-name>
		<type>knob</type>
		<property>/controls/electrical/alternator-2-switch</property>
		<factor>-30</factor>
		<drag-scale-px>5</drag-scale-px>
		<center>
			<x-m>0.054366</x-m>
			<y-m>0.046336</y-m>
			<z-m>0.079196</z-m>
		</center>
		<axis>
			<y>1</y>
		</axis>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/alternator-2-switch</property>
                <min>-1</min>
                <max>1</max>
            </binding>
        </action>
		<release>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/alternator-2-switch</property>
                <step>0</step>
                <min>-1</min>
                <max>0</max>
            </binding>
        </release>
	</animation>
</PropertyList>

