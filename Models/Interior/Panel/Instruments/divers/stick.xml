<?xml version="1.0"?>

	<PropertyList>

	<path>Jag-FS.ac</path>

	<animation>
		<type>rotate</type>
		<object-name>stand</object-name>
		<object-name>Jag-FS</object-name>
		<object-name>Trim</object-name>
		<object-name>Toggle</object-name>
		<object-name>Left-Button</object-name>
		<object-name>Low-Button</object-name>
		<object-name>Pivot</object-name>
		<object-name>Wings</object-name>
		<object-name>screws</object-name>
		<property>controls/flight/elevator</property>
		<factor>8</factor>
		<center>
			<x-m> 0.01236</x-m>
			<y-m> 0.00000</y-m>
			<z-m>-0.02560</z-m>
		</center>
		<axis>
             <y>-1</y> 
         </axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>Jag-FS</object-name>
		<object-name>Trim</object-name>
		<object-name>Toggle</object-name>
		<object-name>Left-Button</object-name>
		<object-name>Low-Button</object-name>
		<object-name>Pivot</object-name>
		<object-name>Wings</object-name>
		<object-name>screws</object-name>
		<factor>-8</factor>
		<property>controls/flight/aileron</property>
		<center>
			<x-m> 0.02625</x-m>
			<y-m>-0.00078</y-m>
			<z-m> 0.21748</z-m>
        </center>
		<axis>
            <x>1</x>
        </axis>
	</animation>

 <animation>
  <type>translate</type>
		<object-name>stand</object-name>
		<object-name>Jag-FS</object-name>
		<object-name>Trim</object-name>
		<object-name>Toggle</object-name>
		<object-name>Left-Button</object-name>
		<object-name>Low-Button</object-name>
		<object-name>Pivot</object-name>
		<object-name>Wings</object-name>
		<object-name>screws</object-name>
        <object-name>Boot</object-name>
  <property>controls/Jag-FS/fore-aft-adjust</property>
  <factor>-0.14</factor>
  <time-sec>0.05</time-sec>
  <offset-m>0</offset-m>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

  <animation>
    <type>pick</type>
    <visible>true</visible>
		<object-name>Boot</object-name>
    <action>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
  <property>controls/Jag-FS/fore-aft-adjust</property>
  <time-sec>0.05</time-sec>
      </binding>
    </action>
  </animation>


	<!-- Lighting -->
	<animation> 
		<object-name>Jag-FS</object-name>
         <object-name>Boot</object-name>
		<type>material</type>
    <emission>
      <red>   0.9 </red>
      <green> 0.9 </green>
      <blue>  0.9 </blue>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
    </emission>
	</animation>


	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>35</max-m>
	</animation>

</PropertyList>
