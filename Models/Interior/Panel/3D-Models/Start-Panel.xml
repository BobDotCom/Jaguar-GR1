<animation>
 <type>select</type> 
 <object-name>LP-Shaft-L-ON</object-name>
 <condition>		
  <greater-than>		
    <property>engines/engine/running</property>
   <value>0</value>
  </greater-than>
 </condition>
 <emission>	
  <red>1</red>	
  <green>1</green>	
  <blue>1</blue>
 </emission>
 <texture>starter.png</texture>
</animation>

<animation>
 <type>select</type> 
 <object-name>LP-Shaft-L-OFF</object-name>
