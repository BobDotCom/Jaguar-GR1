<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <key n="101">
    <name>e</name>
    <desc>Guns</desc>
    <binding>
      <command>nasal</command>
      <script>guns.fire_MG()</script>
    </binding>
    <mod-up>
      <binding>
        <command>nasal</command>
        <script>guns.stop_MG()</script>
      </binding>
    </mod-up>
  </key>

   <key n="100">
    <name>d</name>
    <desc>canopy</desc>
	<binding>
		<command>property-assign</command>
		<property>/fdm/jsbsim/fcs/canopy/engage</property>
		<value>1</value>
	</binding>
  </key>

  <key n="68">
    <name>D</name>
    <desc>close-canopy</desc>
    <binding>
		<command>property-assign</command>
		<property>/fdm/jsbsim/fcs/canopy/engage</property>
		<value>0</value>
	</binding>
  </key>
 
         <key n="76">
            <name>L</name>
            <desc>Deploy slats</desc>
            <repeatable>false</repeatable>
            <binding>
                <command>property-cycle</command>
                <property>fdm/jsbsim/fcs/slats-cmd-deg</property>
                <value type="int">0</value>
                <value type="int">9</value>
                <value type="int">23</value>
                <wrap>false</wrap>
            </binding>
        </key>

         <key n="108">
            <name>l</name>
            <desc>Retract slats</desc>
            <repeatable>false</repeatable>
            <binding>
                <command>property-cycle</command>
                <property>fdm/jsbsim/fcs/slats-cmd-deg</property>
                <value type="int">23</value>
                <value type="int">9</value>
                <value type="int">0</value>
                <wrap>false</wrap>
            </binding>
        </key>
  
  <key n="123">
     <name>}</name>
     <desc>Engine Autostop</desc>
    <binding>
        <command>nasal</command>
        <script>emu.autostop();</script>
    </binding>
  </key>
  
   <key n="125">
     <name>}</name>
     <desc>Engine Autostart</desc>
    <binding>
        <command>nasal</command>
        <script>emu.autostart();</script>
    </binding>
  </key>

